<ul class="list-group">
<%= form_for @todo_list do |f| %>
  <li class="list-group-item" id="todo-list">
    <div class="page-title">
      New Todo List
    </div>
    <div class="new-todo-list-title">
        Title
      <div>
        <%= f.text_field :title, id: "new-todo-list-text" %>
      </div>
    </div>
    <div class="new-todo-list-description">
      <div>
      Description
      </div>
      <div>
      <%= f.text_area :description %>
      </div>
    </div>
  </li>
  <li class="list-group-item" id="create-todo-list">
    <%= f.submit "Create", id: "bottom-btn" %>
  </li>
<% end %>
</ul>
<div class="nav-link">
<%= link_to 'Back', :back %>
</div>